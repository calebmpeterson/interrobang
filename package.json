{
  "name": "zbang",
  "version": "1.0.0",
  "description": "Custom DDG bangs",
  "main": "src/index.js",
  "author": "caleb.peterson@cubicle6.com",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "8.x"
  },
  "scripts": {
    "start": "npx better-npm-run start:development",
    "start:production": "npx better-npm-run start:production",
    "test": "npx mocha",
    "test:watch": "npx mocha --watch",
    "cf:push": "cf push interrobang"
  },
  "betterScripts": {
    "start:development": {
      "command": "nodemon src/index.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:production": {
      "command": "node src/index.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "bootstrap": "4.0.0",
    "bootswatch": "^4.0.0",
    "gists": "^1.0.1",
    "handlebars": "^4.0.11",
    "hapi": "^17.2.0",
    "hapi-react-views": "^10.0.0",
    "inert": "^5.1.0",
    "jquery": "3.2.1",
    "lodash": "^4.17.5",
    "mdi": "^2.1.19",
    "popper.js": "1.12.9",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "vision": "^5.3.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.0.1",
    "nodemon": "^1.15.0"
  }
